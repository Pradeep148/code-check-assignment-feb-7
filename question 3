str = input("Enter a position : ")
if(str[0] == 'a' or str[0] == 'c' or str[0] == 'e' or r[0] == 'g'):
   if(str[1] == '1' or str[1] == '3' or str[1] == '5' or str[1] == '7'):
      print("White")
