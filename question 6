x = int(input("Enter 1st number : "))
y = int(input("Enter 2nd number : "))
z = int(input("Enter 3rd number : "))
if (x == y == z):
  print("All the given numbers are same")
elif (x != y and y == z) :
   print("2nd and 3rd numbers are same")
elif (y != z and x == z) :
   print("1st and 3rd numbers are same")
elif (x != z and y == x) :
   print("1st and 2nd numbers are same")
else :
    print (x , "," , y , "," , z , "are different")
